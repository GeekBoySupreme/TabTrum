{
    "manifest_version": 2,
    "name": "TabTrum",
    "version": "1.0.0",
    "description": "Save & Launch your Browser Tab Combinations in a blink.",
    "permissions": ["tabs", "storage",  "<all_urls>"],

    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "tabtrum_128.png"
        }
    }
  }